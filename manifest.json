{
  "manifest_version": 2,

  "name": "Mint Split",
  "description": "Conveniently split Mint transactions in half",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["https://mint.intuit.com/*"],
      "js": ["mint.js"]
    }
  ],

  "web_accessible_resources": ["images/split.png"]
}